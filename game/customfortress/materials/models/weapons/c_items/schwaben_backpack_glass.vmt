"VertexlitGeneric"
{
	"$basetexture"	"models\weapons\c_items\schwaben_backpack_glass"
//	"$bumpmap" "models/weapons/c_items/c_urinejar_glass_normal"
//	$translucent 1
	"$additive"	"1"
//	"$nocull" 1
//	$halflambert 1

	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .3]"

	"$phong" "1"
	"$phongexponent" "65"
	"$phongfresnelranges" "[1 2 4]"
	"$lightwarptexture" "models\lightwarps\jarate_lightwarp"
	"$phongboost" "6"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" "2"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
